        "test.watch": "stencil test --spec --e2e --watchAll",
        "prettier": "prettier --write **/*.*"
    },
    "devDependencies": {
        "@stencil/core": "^1.0.7"
    },
    "license": "MIT",
    "dependencies": {
        "@stencil/sass": "^1.0.1",
        "bootstrap": "^4.3.1",
        "prettier": "^1.18.2"
